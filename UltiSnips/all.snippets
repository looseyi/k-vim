# Global snippets

snippet Copy
Copyright (c) ${1:`!v g:snips_author`} <${2:`!v g:snips_author_email`}>. All Rights Reserved.
$0
endsnippet



snippet date
`!v strftime("%Y-%m-%d %H:%M:%S")`
endsnippet



snippet datez
`!v strftime("%Y-%m-%d %H:%M:%S %z")`
endsnippet



snippet ts
vim: set ts=${1:4} sw=${2:$1}${3: MORE OPTIONS}:$0
endsnippet



snippet ddate
`!v strftime("%B %d, %Y")`
endsnippet

snippet sdate
`!v strftime("%Y.%m.%d")`
endsnippet

snippet blog
Title: ${1}
Slug: ${2}
Date: ${3}
Category: ${4}
Tags: ${5}

endsnippet


snippet /td
/* TODO: ${1}
*/
endsnippet

snippet /mk
/* MARK: ${1}
*/
endsnippet

snippet mko
${_(u'')}${1}
endsnippet

snippet mkl
' + gettext("") + '${1}
endsnippet

snippet rms
# TODO: remove this
endsnippet

snippet nqa
# noqa
endsnippet

snippet xcheader "XCode File Header"
//
//  ${1:filename}.swift
//  $2
//
//  Created by Edmond on `!v strftime("%Y/%m/%d %H:%M:%S")`
//  Copyright Â© 2015 XueQiu. All rights reserved.
//
endsnippet

snippet	block "rspec: context block"
func $1() {
	${0:${VISUAL:$ assertions...}}
}
endsnippet
